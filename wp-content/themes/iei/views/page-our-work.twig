{% extends "base.twig" %}

{% block content %}
	{% for member in members %}
		<div class="row {{member.post_name}}">
			<div class="media">
				<div class="media-left">
						<img class="media-object img-responsive" src="{{member.thumbnail}}" alt="{{post.title}}">
				</div>
				<div class="media-body">
					<h4 class="media-heading">{{member.title}}</h4>
					{% if member.custom["wpcf-iei-title"] %}<h5 class="media-heading">{{member.custom["wpcf-iei-title"]}}, Innovative Education International, Inc.</h5>{% endif %}
					<h5 class="media-heading">{{member.custom["wpcf-career-title"]}}</h5>
					{{member.content}}
				</div>
			</div>		
		</div>
	{% endfor %}
{% endblock %}