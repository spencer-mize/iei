{% extends "base.twig" %}

{% block content %}
	{% for post in partners %}
		<div class="media">
			<div class="media-left">
					<a href="#"><img class="media-object img-responsive" src="{{post.thumbnail}}" alt="{{post.title}}"></a>
				</div>
			<div class="media-body">
				<h4 class="media-heading">{{post.title}}</h4>
				{{post.content}}
			</div>
		</div>		
	{% endfor %}
{% endblock %}